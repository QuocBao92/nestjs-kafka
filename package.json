  "scripts": {
    "prebuild": "rimraf dist",
    "build:copy": "cpx \"./src/assets/**/*\" \"./dist/assets\"",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "release": "run-s lint test:ci test:e2e-ci build",
    "test": "jest --watchAll --colors --no-cache",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:ci": "jest --bail --no-colors --ci --detectOpenHandles --no-cache",
    "test:e2e": "jest --config ./test/jest-e2e.json --watchAll --no-cache",
    "test:e2e-ci": "jest --config ./test/jest-e2e.json --bail --no-colors --ci --detectOpenHandles --no-cache"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",

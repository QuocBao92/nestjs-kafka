    "class-validator": "^0.13.1",
    "connect-redis": "^5.1.0",
    "express-session": "^1.17.1",
    "kafkajs": "^1.15.0",
    "nestjs-redis": "^1.2.8",
    "nestjs-session": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "uuid": "^8.3.2"
